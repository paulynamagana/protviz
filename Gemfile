gem "jekyll-feed"
gem "jekyll-seo-tag"
gem "jekyll-remote-theme" # This is the plugin that fetches the remote theme
gem "jekyll-last-modified-at"

group :jekyll_plugins do
  # Any plugins that Jekyll needs to load directly
end
